{
	"name": "PIP_SFTP_NIELSEN_FULL_OFFLINE",
	"properties": {
		"description": "- 닐슨 SFTP ",
		"activities": [
			{
				"name": "Copy data",
				"type": "Copy",
				"dependsOn": [
					{
						"activity": "Set yyyymm",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "BinarySource",
						"storeSettings": {
							"type": "SftpReadSettings",
							"recursive": true,
							"disableChunking": false
						},
						"formatSettings": {
							"type": "BinaryReadSettings"
						}
					},
					"sink": {
						"type": "BinarySink",
						"storeSettings": {
							"type": "AzureBlobFSWriteSettings",
							"copyBehavior": "PreserveHierarchy"
						}
					},
					"enableStaging": false
				},
				"inputs": [
					{
						"referenceName": "DTS_NIELSEN_SRC_01",
						"type": "DatasetReference",
						"parameters": {
							"zipname": {
								"value": "@concat('KR_DAESANG_KIMCHI_MT_FullHistory_period_dim',pipeline().parameters.pip_fileName,'.txt.gz')",
								"type": "Expression"
							}
						}
					}
				],
				"outputs": [
					{
						"referenceName": "DTS_NIELSEN_SNK",
						"type": "DatasetReference",
						"parameters": {
							"yyyymm": {
								"value": "@variables('yyyymm')",
								"type": "Expression"
							},
							"dir": {
								"value": "@pipeline().parameters.AdlsDirectory",
								"type": "Expression"
							}
						}
					}
				]
			},
			{
				"name": "Set yyyymm",
				"type": "SetVariable",
				"dependsOn": [],
				"policy": {
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"variableName": "yyyymm",
					"value": {
						"value": "@if(equals(pipeline().parameters.pip_month,null), formatDateTime(addDays(startOfMonth(addHours(utcNow(), 9)),-34),'yyyyMM'), pipeline().parameters.pip_month)",
						"type": "Expression"
					}
				}
			}
		],
		"parameters": {
			"pip_month": {
				"type": "string"
			},
			"pip_fileName": {
				"type": "string"
			},
			"AdlsDirectory": {
				"type": "string",
				"defaultValue": "매출(주문)/외부/유통사MS정보/닐슨_오프라"
			}
		},
		"variables": {
			"yyyymm": {
				"type": "String"
			},
			"filename": {
				"type": "String"
			}
		},
		"folder": {
			"name": "매출_주문/외부/닐슨"
		},
		"annotations": [],
		"lastPublishTime": "2022-12-15T09:05:35Z"
	},
	"type": "Microsoft.DataFactory/factories/pipelines"
}