{
	"name": "PIP_NIELSEN_TO_PG_M",
	"properties": {
		"description": "- 닐슨 SFTP ",
		"activities": [
			{
				"name": "Lookup1",
				"description": "닐슨데이터의 주차 csv 읽기 \nPER_yyyymm.csv",
				"type": "Lookup",
				"dependsOn": [
					{
						"activity": "Set yyyymm",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "DelimitedTextSource",
						"storeSettings": {
							"type": "AzureBlobFSReadSettings",
							"recursive": true,
							"enablePartitionDiscovery": false
						},
						"formatSettings": {
							"type": "DelimitedTextReadSettings"
						}
					},
					"dataset": {
						"referenceName": "DTS_ADLS_SNK",
						"type": "DatasetReference",
						"parameters": {
							"AdlsDirectory": {
								"value": "@concat('/매출(주문)/외부/유통사MS정보/닐슨/',variables('yyyymm'))",
								"type": "Expression"
							},
							"FileName": "PER_202308.csv"
						}
					},
					"firstRowOnly": false
				}
			},
			{
				"name": "Set yyyymm",
				"description": "수행일 기준 전월 구하기\n포멧 : yyyymm",
				"type": "SetVariable",
				"dependsOn": [],
				"policy": {
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"variableName": "yyyymm",
					"value": {
						"value": "@formatDateTime(addDays(startOfMonth(addHours(utcNow(), 9)),-1),'yyyyMM')",
						"type": "Expression"
					}
				}
			},
			{
				"name": "Set minWeek",
				"description": "닐슨에서전송해온 데이터의 시작주차",
				"type": "SetVariable",
				"dependsOn": [
					{
						"activity": "Lookup1",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"policy": {
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"variableName": "minWeek",
					"value": {
						"value": "@activity('Lookup1').output.value[0].TAG",
						"type": "Expression"
					}
				}
			},
			{
				"name": "Set MaxWeek",
				"description": "닐슨에서 가져온 데이터의 마지막주",
				"type": "SetVariable",
				"dependsOn": [
					{
						"activity": "Lookup1",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"policy": {
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"variableName": "maxWeek",
					"value": {
						"value": "@activity('Lookup1').output.value[add(activity('Lookup1').output.count,-1)].TAG",
						"type": "Expression"
					}
				}
			},
			{
				"name": "ㅇ",
				"type": "Copy",
				"dependsOn": [
					{
						"activity": "Set MaxWeek",
						"dependencyConditions": [
							"Succeeded"
						]
					},
					{
						"activity": "Set minWeek",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "DelimitedTextSource",
						"storeSettings": {
							"type": "AzureBlobFSReadSettings",
							"recursive": true,
							"enablePartitionDiscovery": false
						},
						"formatSettings": {
							"type": "DelimitedTextReadSettings"
						}
					},
					"sink": {
						"type": "AzurePostgreSQLSink",
						"writeBatchSize": 1000000,
						"writeBatchTimeout": "00:30:00",
						"writeMethod": "CopyCommand"
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"typeConversion": true,
						"typeConversionSettings": {
							"allowDataTruncation": true,
							"treatBooleanAsNumber": false
						}
					}
				},
				"inputs": [
					{
						"referenceName": "DTS_ADLS_SNK",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "DTS_POSTGRESQL_SNK",
						"type": "DatasetReference"
					}
				]
			}
		],
		"parameters": {
			"ConfFile": {
				"type": "string",
				"defaultValue": "닐슨파일_PG맵.csv"
			}
		},
		"variables": {
			"yyyymm": {
				"type": "String"
			},
			"minWeek": {
				"type": "String"
			},
			"maxWeek": {
				"type": "String"
			}
		},
		"folder": {
			"name": "매출_주문/외부/닐슨"
		},
		"annotations": [],
		"lastPublishTime": "2022-12-15T09:05:35Z"
	},
	"type": "Microsoft.DataFactory/factories/pipelines"
}