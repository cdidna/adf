		"activities": [
			{
				"name": "Lookup1",
				"description": "닐슨데이터의 주차 csv 읽기 \nPER_yyyymm.csv",
				"type": "Lookup",
				"dependsOn": [
					{
						"activity": "Set yyyymm",
						"dependencyConditions": [
							"Succeeded"
						]
					},
					{
						"activity": "file name",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "DelimitedTextSource",
						"storeSettings": {
							"type": "AzureBlobFSReadSettings",
							"recursive": true,
							"enablePartitionDiscovery": false
						},
						"formatSettings": {
							"type": "DelimitedTextReadSettings"
						}
					},
					"dataset": {
						"referenceName": "DTS_ADLS_SNK",
						"type": "DatasetReference",
						"parameters": {
							"AdlsDirectory": {
								"value": "@concat(pipeline().parameters.AdlsDirectory,'/',variables('yyyymm'))",
								"type": "Expression"
							},
							"FileName": {
								"value": "@concat('PER_',variables('fileNm'),'.csv')",
								"type": "Expression"
							}
						}
					},
					"firstRowOnly": false
				}
			},
			{
				"name": "Set yyyymm",
				"description": "수행일 기준 전월 구하기\n포멧 : yyyymm",
				"type": "SetVariable",
				"dependsOn": [],
				"policy": {
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"variableName": "yyyymm",
					"value": {
						"value": "@if(equals(pipeline().parameters.pip_yyyymm,null), formatDateTime(addDays(startOfMonth(addHours(utcNow(), 9)),-34),'yyyyMM'),pipeline().parameters.pip_yyyymm)",
						"type": "Expression"
					}
				}
			},
			{
				"name": "Set minMonth",
				"description": "닐슨에서전송해온 데이터의 시작주차",
				"type": "SetVariable",
				"dependsOn": [
					{
						"activity": "Lookup1",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"policy": {
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"variableName": "minMonth",
					"value": {
						"value": "@activity('Lookup1').output.value[0].TAG",
						"type": "Expression"
					}
				}
			},
			{
				"name": "Set MaxMonth",
				"description": "닐슨에서 가져온 데이터의 마지막주",
				"type": "SetVariable",
				"dependsOn": [
					{
						"activity": "Lookup1",
						"dependencyConditions": [
							"Succeeded"
						]
					}
