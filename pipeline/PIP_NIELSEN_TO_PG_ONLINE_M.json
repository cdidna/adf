					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"variableName": "maxMonth",
					"value": {
						"value": "@activity('Lookup1').output.value[add(activity('Lookup1').output.count,-1)].TAG",
						"type": "Expression"
					}
				}
			},
