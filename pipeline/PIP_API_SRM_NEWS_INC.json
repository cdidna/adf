				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "AzurePostgreSqlSource",
						"query": {
							"value": "select a.gubun   \n      ,a.reason \n      ,a.keyword\n      ,string_agg(a.include_keyword,',') as include_keyword\n      ,a.exclude_keyword \n      ,string_agg(a.include_keyword,'||') as api_include_keyword\n      , case when replace(a.exclude_keyword ,',','||') = '' then '%23%40VK%23S1%23%EC%A0%95%EC%B9%98'\n            else replace(a.exclude_keyword ,',','||')  || '%23%40VK%23S1%23%EC%A0%95%EC%B9%98'\n            end as api_exclude_keyword_new\n      ,replace(a.exclude_keyword ,',','||') as api_exclude_keyword\n  from (\n\t\tselect gubun   \n\t\t      ,reason \n\t\t      ,keyword\t\t    \n\t\t      ,exclude_keyword \n\t\t      ,case when regexp_match( include_keyword,',') is not null then '('||replace(include_keyword,',','&&')||')'\n\t\t            else include_keyword\n\t\t        end include_keyword         \n\t\t  from cd_ods.tb_srm_news_keyword\n       ) a              \ngroup by  a.gubun   \n      ,a.reason \n      ,a.keyword  \n       ,a.exclude_keyword \norder by a.gubun, a.reason, a.keyword\n;",
							"type": "Expression"
						},
						"partitionOption": "None"
					},
					"dataset": {
						"referenceName": "DTS_POSTGRESQL_SNK",
						"type": "DatasetReference",
						"parameters": {
							"SinkSchema": "1",
							"SinkTable": "1"
						}
