{
	"name": "PIP_D_CD_GODOMALL_SUB",
	"properties": {
		"activities": [
			{
				"name": "set strdt",
				"description": "당일자",
				"type": "SetVariable",
				"dependsOn": [],
				"policy": {
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"variableName": "strdt",
					"value": {
						"value": "@if(equals(pipeline().parameters.pip_strdt,null),formatDateTime(addDays(addHours(utcNow(), 9),-10),'yyyyMMdd'),pipeline().parameters.pip_strdt)",
						"type": "Expression"
					}
				}
			},
			{
				"name": "set condition",
				"description": "초기 적재 파이프라인 PIP_D_CD_GODOMALL_INIT 호출시 \n증분주기값 넘어옴",
				"type": "SetVariable",
				"dependsOn": [],
				"policy": {
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [
					{
						"name": "condition"
					}
				],
				"typeProperties": {
					"variableName": "condition",
					"value": {
						"value": "@if(equals(pipeline().parameters.period,null),'1=1',concat('a.period = ',pipeline().parameters.period))",
						"type": "Expression"
					}
				}
			}
		],
		"parameters": {
			"pip_strdt": {
				"type": "string"
			},
			"pip_enddt": {
				"type": "string"
			},
			"period": {
				"type": "string"
			}
		},
		"variables": {
			"strdt": {
				"type": "String"
			},
			"enddt": {
				"type": "String"
			},
			"condition": {
				"type": "String"
			}
		},
		"folder": {
			"name": "고객/내부/고도몰"
		},
		"annotations": []
	}
}