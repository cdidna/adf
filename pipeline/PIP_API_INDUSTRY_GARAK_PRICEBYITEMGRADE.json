						"query": " WITH v_seq AS \r\n (\r\n \tselect   generate_series( '17', '28', 1  ) AS num \r\n )\r\n select distinct to_char(to_date('20230901','YYYYMMDD') + interval '1 day' * v_seq.num,'YYYYMMDD') as date  ,\r\n to_char(to_date('20230901','YYYYMMDD')-1 + interval '1 day' * v_seq.num,'YYYYMMDD') as date_p,\r\n to_char(to_date('20230901','YYYYMMDD')-7 + interval '1 day' * v_seq.num,'YYYYMMDD') as date_p7,'1' as p_pos_gubun\r\nFROM v_seq\r\nunion\r\nselect distinct to_char(to_date('20230901','YYYYMMDD') + interval '1 day' * v_seq.num,'YYYYMMDD') as date  ,\r\n to_char(to_date('20230901','YYYYMMDD')-1 + interval '1 day' * v_seq.num,'YYYYMMDD') as date_p,\r\n to_char(to_date('20230901','YYYYMMDD')-7 + interval '1 day' * v_seq.num,'YYYYMMDD') as date_p7,'3' as p_pos_gubun\r\nFROM v_seq\r\norder by date;\t",
