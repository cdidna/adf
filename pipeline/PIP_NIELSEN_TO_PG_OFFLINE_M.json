									"name": "fact_description",
									"type": "String",
									"physicalType": "character varying"
								}
							},
							{
								"source": {
									"name": "fact_group",
									"type": "String",
									"physicalType": "String"
								},
								"sink": {
									"name": "fact_group",
									"type": "String",
									"physicalType": "character varying"
								}
							},
							{
								"source": {
									"name": "fact_subgroup",
									"type": "String",
									"physicalType": "String"
								},
								"sink": {
									"name": "fact_subgroup",
									"type": "String",
									"physicalType": "character varying"
								}
							},
							{
								"source": {
									"name": "month",
									"type": "String"
								},
								"sink": {
									"name": "ifyyyymm",
									"type": "String",
									"physicalType": "character varying"
								}
							}
						],
						"typeConversion": true,
						"typeConversionSettings": {
							"allowDataTruncation": true,
							"treatBooleanAsNumber": false
						}
					}
				},
				"inputs": [
					{
						"referenceName": "DTS_ADLS_SNK",
						"type": "DatasetReference",
						"parameters": {
							"AdlsDirectory": {
								"value": "@concat(pipeline().parameters.AdlsDirectory,'/',variables('yyyymm'))",
								"type": "Expression"
							},
							"FileName": {
								"value": "@concat('KR_DAESANG_KIMCHI_MT_FullHistory_facts_ref_',variables('fileNm'),'.txt')",
								"type": "Expression"
							}
						}
					}
				],
				"outputs": [
