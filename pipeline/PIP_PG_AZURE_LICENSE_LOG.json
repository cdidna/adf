{
	"name": "PIP_PG_AZURE_LICENSE_LOG",
	"properties": {
		"activities": [
			{
				"name": "Copy_User",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "RestSource",
						"httpRequestTimeout": "00:01:40",
						"requestInterval": "00.00:00:00.010",
						"requestMethod": "GET",
						"paginationRules": {
							"AbsoluteUrl": {
								"value": "$['@odata.nextLink']",
								"type": "Expression"
							}
						}
					},
					"sink": {
						"type": "AzurePostgreSqlSink",
						"writeBatchSize": 1000000,
						"writeBatchTimeout": "00:30:00",
						"writeMethod": "CopyCommand"
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"path": "$['value'][0]['displayName']"
								},
								"sink": {
									"name": "display_name",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['value'][0]['surname']"
								},
								"sink": {
									"name": "surname",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['value'][0]['givenName']"
								},
								"sink": {
									"name": "given_name",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['value'][0]['userPrincipalName']"
								},
								"sink": {
									"name": "user_id",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['value'][0]['companyName']"
								},
								"sink": {
									"name": "company_name",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['value'][0]['employeeId']"
								},
								"sink": {
									"name": "employee_id",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['date']"
								},
								"sink": {
									"name": "snapshot_date",
									"type": "DateTime"
								}
							}
						],
						"collectionReference": "",
						"mapComplexValuesToString": false
					}
				},
				"inputs": [
					{
						"referenceName": "AZURE_USER_INFO",
						"type": "DatasetReference",
						"parameters": {
							"url": "/v1.0/users?$select=displayName,surname,givenName,userPrincipalName,companyName,employeeId"
						}
					}
				],
				"outputs": [
					{
						"referenceName": "DTS_POSTGRESQL_SNK",
						"type": "DatasetReference",
						"parameters": {
							"SinkSchema": "cd_ods",
							"SinkTable": "tb_azure_user_info"
						}
					}
				]
			}
		],
		"parameters": {
			"date": {
				"type": "string",
				"defaultValue": "@{formatDateTime(utcNow(),'yyyy-MM-dd')} "
			}
		},
		"folder": {
			"name": "사용자/내부"
		},
		"annotations": []
	}
}