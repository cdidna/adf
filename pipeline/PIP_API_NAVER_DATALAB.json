{
	"name": "PIP_API_NAVER_DATALAB",
	"properties": {
		"activities": [
			{
				"name": "ForEach1",
				"type": "ForEach",
				"dependsOn": [
					{
						"activity": "select_keyword",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"userProperties": [],
				"typeProperties": {
					"items": {
						"value": "@activity('select_keyword').output.value",
						"type": "Expression"
					},
					"isSequential": true,
					"activities": [
						{
							"name": "Copy data1_copy1",
							"type": "Copy",
							"dependsOn": [],
							"policy": {
								"timeout": "0.12:00:00",
								"retry": 0,
								"retryIntervalInSeconds": 30,
								"secureOutput": false,
								"secureInput": false
							},
							"userProperties": [],
							"typeProperties": {
								"source": {
									"type": "JsonSource",
									"additionalColumns": [
										{
											"name": "key_id",
											"value": {
												"value": "@item().key_id",
												"type": "Expression"
											}
										},
										{
											"name": "topic",
											"value": {
												"value": "@item().topic",
												"type": "Expression"
											}
										}
									],
									"storeSettings": {
										"type": "HttpReadSettings",
										"requestMethod": "POST",
										"additionalHeaders": {
											"value": "Content-Type: application/json; UTF-8\nX-Naver-Client-Id : @{item().id}\nX-Naver-Client-Secret : @{item().pw}",
											"type": "Expression"
										},
										"requestBody": {
											"value": "{\n  \"startDate\": \"@{item().start_date}\",\n  \"endDate\": \"@{item().end_date}\",\n  \"timeUnit\": \"month\",\n  \"keywordGroups\": [\n    {\n      \"groupName\": \"@{item().topic}\",\n      \"keywords\": [\"@{item().topic}\"]\n    }\n  ]\n}",
											"type": "Expression"
										},
										"requestTimeout": ""
									},
									"formatSettings": {
										"type": "JsonReadSettings"
									}
								},
								"sink": {
									"type": "AzurePostgreSqlSink",
									"writeBatchSize": 1000000,
									"writeBatchTimeout": "00:30:00",
									"writeMethod": "CopyCommand"
								},
								"enableStaging": false,
								"translator": {
									"type": "TabularTranslator",
									"mappings": [
										{
											"source": {
												"path": "['data'][*]['period']"
											},
											"sink": {
												"name": "period",
												"type": "String"
											}
										},
										{
											"source": {
												"path": "['data'][*]['ratio']"
											},
											"sink": {
												"name": "ratio",
												"type": "String"
											}
										},
										{
											"source": {
												"path": "$['key_id']"
											},
											"sink": {
												"name": "key_id",
												"type": "String"
											}
										},
										{
											"source": {
												"path": "$['topic']"
											},
											"sink": {
												"name": "topic",
												"type": "String"
											}
										}
									],
									"collectionReference": "$['results']"
								}
							},
							"inputs": [
								{
									"referenceName": "DTS_NAVER_DATALAB_API",
									"type": "DatasetReference"
								}
							],
							"outputs": [
								{
									"referenceName": "DTS_POSTGRESQL_SNK",
									"type": "DatasetReference",
									"parameters": {
										"SinkSchema": {
											"value": "@pipeline().parameters.SinkSckema",
											"type": "Expression"
										},
										"SinkTable": {
											"value": "@pipeline().parameters.SinkTable",
											"type": "Expression"
										}
									}
								}
							]
						}
					]
				}
			},
			{
				"name": "select_keyword",
				"type": "Lookup",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 5,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "AzurePostgreSqlSource",
						"query": {
							"value": "WITH id_pw_combinations AS (\n  SELECT \n    ROW_NUMBER() OVER () as combo_id,\n    id, \n    pw\n  FROM (\n    VALUES \n      ('p7kCTjIvjnAYEJGAvsl0', 'fabaTiAHhF'),\n      ('YGhFoYAuLjwOeggaogEg', '2fBbQxicmX'),\n      ('JKMVVBdEU58bv9gVHyeh', 'spp0sh3TAq')\n  ) AS combinations(id, pw)\n),\nbase_data AS (\n  SELECT \n    topic,\n    key_id,\n    to_char(DATE_TRUNC('month', current_date - interval '6 month'), 'YYYY-MM-DD') as start_date,\n     to_char(now() - interval '1 day', 'YYYY-MM-DD') as end_date,\n    ROW_NUMBER() OVER () as row_num\n  FROM cd_portal.tb_topic_master\n  where use_yn = 'Y'\nand new_traker  = 'N'\n)\nSELECT \n  bd.topic,\n  bd.key_id,\n  bd.start_date,\n  bd.end_date,\n  ipc.id,\n  ipc.pw\nFROM base_data bd\nJOIN id_pw_combinations ipc ON ipc.combo_id = ((bd.row_num - 1) % 3) + 1",
							"type": "Expression"
						},
						"partitionOption": "None"
					},
					"dataset": {
						"referenceName": "DTS_POSTGRESQL_SNK",
						"type": "DatasetReference",
						"parameters": {
							"SinkSchema": "1",
							"SinkTable": "1"
						}
					},
					"firstRowOnly": false
				}
			}
		],
		"parameters": {
			"SinkTable": {
				"type": "string",
				"defaultValue": "tb_naver_datalab_keyword_t"
			},
			"SinkSckema": {
				"type": "string",
				"defaultValue": "cd_ods"
			}
		},
		"variables": {
			"id": {
				"type": "Integer"
			},
			"pw": {
				"type": "String"
			}
		},
		"folder": {
			"name": "고객/외부/홈대시보드"
		},
		"annotations": []
	}
}